{
  "name": "Youtube Sponsorship Skipper",
  "description": "Helps you skip YouTube sponsorships",
  "version": "1.3",
  "manifest_version": 3,
  "permissions": [
    "tabs",
    "scripting",
    "identity",
    "storage",
    "webRequest",
    "identity.email"
  ],
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "32": "assest/5.png"
    }
  },
  "icons": {
    "16": "assest/5.png",
    "32": "assest/5.png",
    "48": "assest/5.png",
    "128": "assest/5.png"
  },
  "content_scripts": [
    {
      "js": [
        "contentScript.js"
      ],
      "matches": [
        "https://*.youtube.com/*",
        "https://youtube-sponsership-skipper.vercel.app/"
      ]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "helper.js",
        "service-worker.js",
        "getCaptions.js",
        "config.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "background": {
    "service_worker": "service-worker.js",
    "type": "module"
  },
  "key": "",
  "oauth2": {
    "client_id": "6327844063-4jlvcq9c71plg9fnfdobet4elht7vko2.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/userinfo.email",
      "https://www.googleapis.com/auth/userinfo.profile"
    ]
  },
  "host_permissions": [
    "https://*.youtube.com/*",
    "https://youtube-sponsership-skipper.vercel.app/",
    "https://ytsponsershipskipper-production.up.railway.app/"
  ]
}
