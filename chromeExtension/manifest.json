{
  "name": "Youtube Sponsorship Skipper",
  "description": "Helps you skip YouTube sponsorships",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": [
    "activeTab",
    "tabs",
    "scripting",
    "identity",
    "storage",
    "identity.email",
    "unlimitedStorage"
  ],
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "32": "assest/5.png"
    }
  },
  "icons": {
    "16": "assest/5.png",
    "32": "assest/5.png",
    "48": "assest/5.png",
    "128": "assest/5.png"
  },
  "content_scripts": [
    {
      "js": [
        "contentScript.js"
      ],
      "matches": [
        "https://*.youtube.com/*",
        "<all_urls>"
      ]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "helper.js",
        "service-worker.js",
        "getCaptions.js",
        "config.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "background": {
    "service_worker": "service-worker.js",
    "type": "module"
  },
  "key": "",
  "oauth2": {
    "client_id": "536289115131-miueeek1eloehmb3cav6svdv21icpskt.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/userinfo.email",
      "https://www.googleapis.com/auth/userinfo.profile"
    ]
  },
  "host_permissions": [
    "http://localhost:8080/*",
    "https://*/*"
  ]
}
